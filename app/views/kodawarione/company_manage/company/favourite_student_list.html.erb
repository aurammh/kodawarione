<!--begin::Header-->
<% breadcrumbs_navigation_add (t('btn.company_search')), kodawarione_company_manage_company_search_path %>
<% breadcrumbs_navigation_add (t ('common.favourite_users')), '#' %>
<%= render_partner_breadcrumbs_navigation(t ('student.title.header_link1')) %>			
<!--end::Header-->
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
	<!--begin::Container-->
	<div class="container-fluid" id="kt_content_container">
		<div>
			<%= render partial: "kodawarione/shared/company_manage/profile_navbar", :locals =>{:current_route => "favourite_student_list"}  %>
		</div>
		<!--begin::Tab Content-->
		<div class="tab-content">
			<!--begin::Tab pane-->
			<div id="kt_project_users_card_pane" class="tab-pane fade show active">
				<% if can?(:favourite_student_list, Kodawarione::CompanyManage) %>
                <%= render partial: "common/favourite_student_list", locals: { fav_students: @favourite_users_list } %>  
				<% end %>
            </div>
        </div>
    </div>
</div>