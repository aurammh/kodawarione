                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <%= form.text_area :address, class: "form-control form-control-lg form-control-solid", rows: 4, maxlength: 255, id: "address" %>
                                        <p class="text-danger fs-6 ms-invalid-feedback mt-1"><%= @company_details.errors.full_messages_for(:address)[0] %></p>
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div class="row mb-6 display_address">
                                    <!--begin::Label-->
                                    <%= form.label :display_address, class: "col-lg-4 col-form-label  fw-bold fs-6" %>
