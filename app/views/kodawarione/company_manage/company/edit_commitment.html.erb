                                        </div>
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <%= form.label :website_url, class: "col-lg-4 col-form-label fw-bold fs-6 required" %>
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <%= form.text_field :website_url, class: "form-control form-control-lg form-control-solid", id: "website_url",  maxlength: 255 %>
                                        <p class="text-danger fs-6 ms-invalid-feedback mt-1"><%= @company_details.errors.full_messages_for(:website_url)[0] %></p>
