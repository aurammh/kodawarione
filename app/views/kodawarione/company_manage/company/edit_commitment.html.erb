                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <%= form.label :postal_code, class: "col-lg-4 col-form-label required fw-bold fs-6" %>
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <%= form.text_field :postal_code, class: "form-control form-control-lg form-control-solid", autocomplete: "off", maxlength: 7,id: "postalcode" %>
                                        <p class="text-danger fs-6 ms-invalid-feedback mt-1"><%= @company_details.errors.full_messages_for(:postal_code)[0] %></p>    
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div>
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <%= form.hidden_field :m_region_id,value: @company_details.m_region_id ,id: "region_id" %>
                                        <%= form.hidden_field :region_name, class: "form-control form-control-lg form-control-solid",id: "region",readonly: true %>
                                        <!--<p class="text-danger fs-6 ms-invalid-feedback mt-1"><%# @company_details.errors.full_messages_for(:m_region_id)[0] %></p>-->
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->                        
                                <!--begin::Input group-->
                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <%= form.label :m_prefecture_id, class: "col-lg-4 col-form-label required fw-bold fs-6" %> 
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <%= form.hidden_field :m_prefecture_id,value: @company_details.m_prefecture_id ,id: "prefecture_id" %>
                                        <%= form.text_field :prefecture_name, class: "form-control form-control-lg form-control-solid",id: "prefecture",readonly: true %>
                                        <p class="text-danger fs-6 ms-invalid-feedback mt-1"><%= @company_details.errors.full_messages_for(:m_prefecture_id)[0] %></p>
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <%= form.label :postalcode_city, class: "col-lg-4 col-form-label required fw-bold fs-6" %>
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <%= form.text_field :postalcode_city, class: "form-control form-control-lg form-control-solid",id: "city", readonly: true%>
                                        <p class="text-danger fs-6 ms-invalid-feedback mt-1"><%= @company_details.errors.full_messages_for(:postalcode_city)[0] %></p>
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <%# postal code end%>
