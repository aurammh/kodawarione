<!--begin::Header-->
<% breadcrumbs_navigation_add (t('title.student_search_detail_title')), kodawarione_company_manage_company_details_overview_path(:company_id => @company_details.id, :status => 1) %>
<% breadcrumbs_navigation_add (t ('title.basic_registration_title')), '#' %>
<%= render_partner_breadcrumbs_navigation(t ('title.basic_registration_title')) %>		
<!--end::Header-->
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
	<!--begin::Container-->
	<div class="container-fluid" id="kt_content_container">
		<!--begin::Tab Content-->
		<div class="tab-content">
			<!--begin::Tab pane-->
			<div id="kt_project_users_card_pane" class="tab-pane fade show active">
            <% if can?(:edit_commitment, Kodawarione::CompanyManage) %>
                <!--begin::Content-->
                <div class="content flex-row-fluid" id="kt_content_container">
                    <!--begin::Navbar-->
                    <%= render partial: "kodawarione/shared/company_manage/profile_banner" %>                     <!--end::Navbar-->
                    <!--begin::Card-->
                    <div class="card bg-body mb-5 mb-xl-10">
                    <!--begin::Card header-->
                    <div class="card-header border-0">
                        <!--begin::Card title-->
                        <div class="card-title m-0">
                            <h3 class="fw-bolder m-0">
                                <% if @company_details.created_at == @company_details.updated_at %>
                                    <%=t ('student_commitment.menu.new_corporate_information_registration')%>
                                <% else %>
                                    <%=t ('title.temporary_registration_title')%>
                                <% end %>
                            </h3>
                        </div>
                        <!--end::Card title-->
                    </div>
                    <!--end::Card header-->
                    <!--begin::Form-->
                    <%= form_with(model: @company_details, url: kodawarione_company_manage_update_commitment_path, method: "post") do |form| %>
                        <%= form.hidden_field :not_company_edit, :value => true %>
                            <!--begin::Card body-->
                            <div class="card-body border-top p-9">
                                <!--begin::Commitment Information-->
                                <!--begin::Input group-->
                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <%= form.label :name, class: "col-lg-4 col-form-label required fw-bold fs-6" %>
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <%= form.text_field :company_name, id: "company_name", class: "form-control form-control-lg form-control-solid text-uppercase", maxlength: 200 %>
                                        <p class ="text-danger fs-6 ms-invalid-feedback mt-1"><%= @company_details.errors.full_messages_for(:company_name)[0] %></p>
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <%= form.label :name_furikana, class: "col-lg-4 col-form-label required fw-bold fs-6" %>
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <%= form.text_field :company_name_kana, id: "company_name_kana", class: "form-control form-control-lg form-control-solid text-uppercase", maxlength: 200 %>
                                        <p class ="text-danger fs-6 ms-invalid-feedback mt-1"><%= @company_details.errors.full_messages_for(:company_name_kana)[0] %></p>
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <label class="col-lg-4 col-form-label required fw-bold fs-6">
                                        <%= form.label :phone_no %><%= form.label t('common.ph_no_remark') %>
                                    </label>
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8 fv-row">
                                        <div class="position-relative d-flex align-items-center">
                                            <!--begin::Icon-->
                                            <!--begin::Svg Icon | path: icons/duotune/general/gen014.svg-->
                                            <span class="svg-icon svg-icon-2 position-absolute mx-4">
