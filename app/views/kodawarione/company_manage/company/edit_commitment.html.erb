                                            <!--end::Icon-->
                                            <%= form.text_field :phone_no, id: "phone_no", class: "only-num-stud form-control form-control-lg form-control-solid ps-12", maxlength: 20 %>
                                        </div>
                                        <p class ="text-danger fs-6 ms-invalid-feedback mt-1"><%= form.object.errors.full_messages_for(:phone_no)[0] %></p>
                                    </div>
                                    <!--end::Col-->
                                </div>
                                <!--end::Input group--> 
                                <%# write for postal code panel company_address %>
                                <%# postal code testing Start %>
