<!--begin::Header-->
<% breadcrumbs_navigation_add (t('super_partner.title.super_partner_list')), kodawarione_super_partner_manage_index_path %>
<% breadcrumbs_navigation_add (t('student.title.header_link1')), kodawarione_super_partner_manage_super_partner_details_path %>
<% breadcrumbs_navigation_add (t('super_partner.title.member_list')), '#' %>
<%= render_partner_breadcrumbs_navigation(t('super_partner.title.member_list')) %>		
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
  <!--begin::Container-->
  <div class="container-fluid" id="kt_content_container">
        <!--begin::Layout-->
		<div class="d-flex flex-column flex-lg-row">
			<!--begin::Content-->
			<div class="flex-lg-row-fluid me-lg-15 order-2 order-lg-1 mb-10 mb-lg-0">
				<!--begin::Card-->
				<div class="card bg-body card-flush pt-3 mb-5 mb-xl-10">
					<!--begin::Card header-->
					<div class="card-header">
                        <!--begin::Card title-->
						<div class="card-title">
							<h2 class="fw-bolder"><%= t('super_partner.title.member_list') %></h2>
						</div>
						<!--begin::Card title-->
						<!--begin::Card toolbar-->
                        <% if can?(:new_super_partner_member_setup, Kodawarione::SuperPartner) %>
                            <div class="card-toolbar">
                                <%= link_to t('super_partner.btn.add_new_member'), kodawarione_super_partner_manage_new_super_partner_member_setup_path, :class=>"btn btn-light-primary" %>
                            </div>
                        <% end %>
						<!--end::Card toolbar-->
					</div>
					<!--end::Card header-->
					<!--begin::Card body-->
					<div class="card-body pt-3">
						<!--begin::Section-->
						<div class="mb-0">
                            <% if @super_partner_members.present?  %>
                                <!--begin::Product table-->
                                <div class="table-responsive">
                                    <!--begin::Table-->
                                    <table class="table align-middle table-row-dashed fs-6 gy-4 mb-0">
                                        <!--begin::Table head-->
                                        <thead>
                                            <!--begin::Table row-->
                                            <tr class="border-bottom border-gray-200 text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0">
                                                <th class="min-w-150px"><%= t('partner.partner_user.title.name') %></th>
                                                <th class="min-w-125px"><%= t('partner.partner_user.title.email') %></th>
                                                <th class="min-w-125px"><%= t('admin.news.news_created_date') %></th>
                                                <th class="text-end min-w-70px"></th>
                                            </tr>
                                            <!--end::Table row-->
                                        </thead>
                                        <!--end::Table head-->
                                        <!--begin::Table body-->
                                        <tbody class="fw-bold text-gray-800">
                                            <% @super_partner_members.each do |super_partner_member| %>
                                                <tr>
                                                    <td>
                                                        <div class="text-break fw-normal w-200px"><%= super_partner_member.last_name+' '+super_partner_member.first_name %></div>
                                                    </td>
                                                    <td>
                                                        <span class="w-250px"><%= super_partner_member.email %></span>
                                                    </td>
                                                    <td><%= l(super_partner_member.created_at.strftime('%Y-%m-%d').to_date, default: '-') %></td>
                                                    <td class="text-end">
                                                        <!--begin::Action-->
                                                        <% if can?(:show_super_partner_member_setup, Kodawarione::SuperPartner) %>
                                                            <%= link_to kodawarione_super_partner_manage_show_super_partner_member_setup_path(id: super_partner_member.member_id, super_partner_id: @super_partner.id ), class: "btn btn-icon btn-active-light-primary w-30px h-30px" do %>
                                                                <!--begin::Svg Icon | path: icons/duotune/general/gen019.svg-->
                                                                <span class="svg-icon svg-icon-3">
                                                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                                                        <path d="M17.5 11H6.5C4 11 2 9 2 6.5C2 4 4 2 6.5 2H17.5C20 2 22 4 22 6.5C22 9 20 11 17.5 11ZM15 6.5C15 7.9 16.1 9 17.5 9C18.9 9 20 7.9 20 6.5C20 5.1 18.9 4 17.5 4C16.1 4 15 5.1 15 6.5Z" fill="black" />
                                                                        <path opacity="0.3" d="M17.5 22H6.5C4 22 2 20 2 17.5C2 15 4 13 6.5 13H17.5C20 13 22 15 22 17.5C22 20 20 22 17.5 22ZM4 17.5C4 18.9 5.1 20 6.5 20C7.9 20 9 18.9 9 17.5C9 16.1 7.9 15 6.5 15C5.1 15 4 16.1 4 17.5Z" fill="black" />
                                                                    </svg>
                                                                </span>
                                                                <!--end::Svg Icon-->
                                                            <% end %>
                                                            <!--end::Action-->
                                                        <% end %>
                                                        <!--begin::Action-->
                                                        <% if can?(:edit_super_partner_member_setup, Kodawarione::SuperPartner) %>
                                                            <%= link_to kodawarione_super_partner_manage_edit_super_partner_member_setup_path(id: super_partner_member.member_id, super_partner_id: @super_partner.id ), class: "btn btn-icon btn-active-light-primary w-30px h-30px" do %>
                                                                <!--begin::Svg Icon | path: icons/duotune/general/gen019.svg-->
                                                                <span class="svg-icon svg-icon-3">
                                                                    <!--begin::Svg Icon | path:assets/media/svg/icons/Communication/Write.svg-->
                                                                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                                                                        <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                                                            <rect x="0" y="0" width="24" height="24" />
                                                                            <path d="M12.2674799,18.2323597 L12.0084872,5.45852451 C12.0004303,5.06114792 12.1504154,4.6768183 12.4255037,4.38993949 L15.0030167,1.70195304 L17.5910752,4.40093695 C17.8599071,4.6812911 18.0095067,5.05499603 18.0083938,5.44341307 L17.9718262,18.2062508 C17.9694575,19.0329966 17.2985816,19.701953 16.4718324,19.701953 L13.7671717,19.701953 C12.9505952,19.701953 12.2840328,19.0487684 12.2674799,18.2323597 Z" fill="#000000" fill-rule="nonzero" transform="translate(14.701953, 10.701953) rotate(-135.000000) translate(-14.701953, -10.701953)" />
                                                                            <path d="M12.9,2 C13.4522847,2 13.9,2.44771525 13.9,3 C13.9,3.55228475 13.4522847,4 12.9,4 L6,4 C4.8954305,4 4,4.8954305 4,6 L4,18 C4,19.1045695 4.8954305,20 6,20 L18,20 C19.1045695,20 20,19.1045695 20,18 L20,13 C20,12.4477153 20.4477153,12 21,12 C21.5522847,12 22,12.4477153 22,13 L22,18 C22,20.209139 20.209139,22 18,22 L6,22 C3.790861,22 2,20.209139 2,18 L2,6 C2,3.790861 3.790861,2 6,2 L12.9,2 Z" fill="#000000" fill-rule="nonzero" opacity="0.3" />
                                                                        </g>
                                                                    </svg>
                                                                    <!--end::Svg Icon-->
                                                                </span>
                                                                <!--end::Svg Icon-->
                                                            <% end %>
                                                        <% end %>
                                                        <!--end::Action-->
                                                        <% if can?(:destroy_super_partner_member, Kodawarione::SuperPartner) %>
                                                            <!--begin::Action-->
                                                            <button type="button" class="btn btn-icon btn-active-light-primary w-30px h-30px" data-bs-toggle="modal" data-bs-target="#delete_news">
                                                                <span class="svg-icon svg-icon-3" data-bs-toggle="modal" data-bs-target="#delete_news">
                                                                    <!--begin::Svg Icon | path:assets/media/svg/icons/General/Trash.svg-->
                                                                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                                                                        <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                                                            <rect x="0" y="0" width="24" height="24" />
                                                                            <path d="M6,8 L6,20.5 C6,21.3284271 6.67157288,22 7.5,22 L16.5,22 C17.3284271,22 18,21.3284271 18,20.5 L18,8 L6,8 Z" fill="#000000" fill-rule="nonzero" />
                                                                            <path d="M14,4.5 L14,4 C14,3.44771525 13.5522847,3 13,3 L11,3 C10.4477153,3 10,3.44771525 10,4 L10,4.5 L5.5,4.5 C5.22385763,4.5 5,4.72385763 5,5 L5,5.5 C5,5.77614237 5.22385763,6 5.5,6 L18.5,6 C18.7761424,6 19,5.77614237 19,5.5 L19,5 C19,4.72385763 18.7761424,4.5 18.5,4.5 L14,4.5 Z" fill="#000000" opacity="0.3" />
                                                                        </g>
                                                                    </svg>
                                                                    <!--end::Svg Icon-->
                                                                </span>
                                                            </button>
                                                            <!--end::Action-->
                                                            <div class="modal fade pl-0" id="delete_news" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                                                <div class="modal-dialog modal-lg">
                                                                    <div class="modal-content ">
                                                                        <div class="modal-header">
                                                                            <h3 class="modal-title" id="exampleModalLongTitle"><%= t('common.modal_confirm_title') %></h3>
                                                                            <div class="btn btn-sm btn-icon btn-active-color-primary" data-bs-dismiss="modal">
                                                                            <!--begin::Svg Icon | path: icons/duotune/arrows/arr061.svg-->
                                                                            <span class="svg-icon svg-icon-1">
                                                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                                                                    <rect opacity="0.5" x="6" y="17.3137" width="16" height="2" rx="1" transform="rotate(-45 6 17.3137)" fill="black" />
                                                                                    <rect x="7.41422" y="6" width="16" height="2" rx="1" transform="rotate(45 7.41422 6)" fill="black" />
                                                                                </svg>
                                                                            </span>
                                                                            <!--end::Svg Icon-->
                                                                            </div>
                                                                        </div>
                                                                        <div class="modal-body company_type">
                                                                            <section>
                                                                                <div class="text-start py-3">
                                                                                    <h5 class="text-dark font-weight-bold pl-1"><%= t('search.admin_delete_msg') %></h5>
                                                                                </div>
                                                                                <div class="mt-3 d-flex justify-content-end">
                                                                                    <%= link_to t('btn.confirm_yes'),{:controller => "super_partners", :action => 'destroy_super_partner_member', :id => super_partner_member.member_id},:method => :delete, class: "btn btn-danger text-white font-weight-bold col-sm-6 col-md-4 col-lg-2 custom-btn px-2 py-2 text-decoration-none ml-4 me-3" %>
                                                                                    <button type="button" class="btn bg-light font-weight-bold col-sm-6 col-md-4 col-lg-2 px-2 py-2 ml-3" data-bs-dismiss="modal"><%= t('btn.cancel') %></button>
                                                                                </div>
                                                                            </section>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        <% end %>
                                                    </td>
                                                </tr>
                                            <% end %>
                                        </tbody>
                                        <!--end::Table body-->
                                    </table>
                                    <!--end::Table-->
                                </div>
                                <div class="d-flex flex-wrap justify-content-between align-items-center pt-10">
                                    <div class="d-flex flex-wrap py-2 mr-3">
                                        <%= paginate @super_partner_members %>
                                    </div>
                                    <div class="d-flex align-items-center py-3">
                                        <span class="text-muted"><%=  t('common.display')  %><%= @super_partner_members.total_count %> <%=  t('common.to')  %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <%= @super_partner_members.offset_value + 1 %> ~ <%=  @super_partner_members.offset_value + @super_partner_members.count %> <%=  t('common.search_result')  %> </span>
                                    </div>
                                </div>
                                <!--end::Product table-->
                            <% else %>
                                <div class="col-12 m-2">
                                    <h3><%= t('search.no_result') %></h3>
                                </div>
                            <% end %>
						</div>
						<!--end::Section-->
					</div>
					<!--end::Card body-->
				</div>
				<!--end::Card-->
			</div>
			<!--end::Content-->
		</div>
		<!--end::Layout-->
	</div>
	<!--end::Post-->
</div>
<!--end::Container-->