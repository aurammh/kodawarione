                        <!--end::Row-->
                        <div id="kt_project_users_card_pane" class="tab-pane fade show active">
				<!--begin::Table-->
				<div class="card bg-body p-10">
					<!--begin::Header-->
                    <div class="card-header border-0 px-0">
                        <h3 class="fw-bolder me-5 my-1">
                            <span class="text-primary me-2"><%= t ('search.search_title') %></span> <%= @news_results.total_count %> <%= t ('search.count_unit') %>
                        </h3>
                    </div>
                    <!--end::Header-->
                    <div class="separator separator-dashed mb-6"></div>
					<!--begin::Body-->
					<div class="row">
                        <% if @news_results.present? %>
                            <% @news_results.each_with_index do |news,index| %>
                                <div class="mb-0">
										<!--begin::Head-->
										<div class="d-flex align-items-center mb-4">
											<!--begin::Title-->
                                            <%= link_to kodawarione_news_path(:id => news.id), class: "fs-2 fw-bolder text-gray-900 text-hover-primary me-1" do %>  
                                                <%= news.title %> 
                                            <% end %>
											<!--end::Title-->
										</div>
										<!--end::Head-->
										<!--begin::Summary-->
										<div class="fs-base fw-normal text-gray-700 mb-4">
                                            <%= news.content %> 
                                        </div>
										<!--end::Summary-->
										<!--begin::Foot-->
										<div class="d-flex flex-stack flex-wrap">
											<!--begin::Author-->
											<div class="d-flex align-items-center py-1">
												<!--begin::Symbol-->
												<%# <div class="symbol symbol-35px me-2">
													<div class="symbol-label bg-light-success fs-3 fw-bold text-success text-uppercase">J</div>
												</div> %>
												<!--end::Symbol-->
												<!--begin::Name-->
												<div class="d-flex flex-column align-items-start justify-content-center px-4 me-2">
													<%# <span class="text-gray-900 fs-7 fw-bold lh-1 mb-2">James Hunt</span> %>
													<span class="text-muted fs-8 fw-bold lh-1"><%= l(news.created_at.strftime('%Y-%m-%d').to_date, default: '-') %></span>

												</div>
												<!--end::Name-->
                                                <span class="btn btn-sm btn-outline btn-outline-dashed btn-outline-default ml-3"><%= news.news_type.nil? ? '' : t("partner_news.news_type_options.#{news.news_category}") %></span>
											</div>
											<!--end::Author-->
											<!--begin::Info-->
											<div class="d-flex align-items-center py-1">
												<!--begin::Tags-->
                                                <%= link_to edit_kodawarione_news_path(id: news.id), :class=> 'btn btn-sm btn-light px-4 me-2' do %>
                                                    <span class="svg-icon svg-icon-3">
                                                                <!--begin::Svg Icon | path:assets/media/svg/icons/Communication/Write.svg-->
                                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
