<!--begin::Header-->
<% breadcrumbs_navigation_add (t('partner_contract.list')), partner_contracts_path %>
<% breadcrumbs_navigation_add (t('partner_contract.contract_information')),'#' %>
<%= render_partner_breadcrumbs_navigation(t('partner_contract.contract_information')) %>
<!--end::Header-->
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
  <!--begin::Container-->
  <div class="container-fluid" id="kt_content_container">
    <!--begin::Post card-->
    <div class="card bg-body">
      <!--begin::Body-->
      <div class="card-body p-lg-10 pb-lg-0">
        <!--begin::Layout-->
        <div class="d-flex flex-column flex-xl-row">
          <!--begin::Content-->
          <div class="flex-lg-row-fluid">
            
            <!--begin::Event details-->
            <div class="row">
              <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-6 my-3">
              <div class="d-flex justify-content-between">
                <div class="text-dark text-hover-primary fs-6 fw-bolder">
                <%= t('partner_contract.contract_information') %></div>
                <%# start ********************************** %>
                <button type="button" class="btn-danger custom-btn px-4 pb-2 pt-1 text-decoration-none" data-bs-toggle="modal" data-bs-target="#delete_news">
                  <%= t('common.profile_delete')%>
                </button>
                                                    <div class="modal fade pl-0" id="delete_news" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                                        <div class="modal-dialog modal-lg">
                                                            <div class="modal-content ">
                                                                <div class="modal-header">
                                                                    <h3 class="modal-title" id="exampleModalLongTitle"><%= t('common.modal_confirm_title') %></h3>
                                                                    <div class="btn btn-sm btn-icon btn-active-color-primary" data-bs-dismiss="modal">
                                                                        <!--begin::Svg Icon | path: icons/duotune/arrows/arr061.svg-->
                                                                        <span class="svg-icon svg-icon-1">
                                                                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                                                                <rect opacity="0.5" x="6" y="17.3137" width="16" height="2" rx="1" transform="rotate(-45 6 17.3137)" fill="black" />
                                                                                <rect x="7.41422" y="6" width="16" height="2" rx="1" transform="rotate(45 7.41422 6)" fill="black" />
                                                                            </svg>
                                                                        </span>
                                                                        <!--end::Svg Icon-->
                                                                    </div>
                                                                </div>
                                                                <div class="modal-body company_type">
                                                                    <section>
                                                                        <div class="text-start py-3">
                                                                            <h5 class="text-dark font-weight-bold pl-1"><%= t('search.admin_delete_msg') %></h5>
                                                                        </div>
                                                                        <div class="mt-3 d-flex justify-content-end">
                                                                            <%= link_to t('btn.confirm_yes'),{:controller => "contracts", :action => 'destroy', :id => @contract.id},:method => :delete, class: "btn btn-danger text-white font-weight-bold col-sm-6 col-md-4 col-lg-2 custom-btn px-2 py-2 text-decoration-none ml-4 me-3" %>
                                                                            <button type="button" class="btn btn-dark text-white font-weight-bold col-sm-6 col-md-4 col-lg-2 px-2 py-2 ml-3" data-bs-dismiss="modal"><%= t('btn.cancel') %></button>
                                                                        </div>
                                                                    </section>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                <%# end ***************************************** %>
                </div>
                <div class="d-flex flex-wrap my-3">
                  <!--begin::Due-->
                  <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-7 mb-3">
                    <div class="fs-6 text-gray-800 fw-bolder"><%= get_super_partner_name(@contract.contract_to_id) %></div>
                    <div class="fw-bold text-gray-600"><%= t('partner_contract.company_id') %></div>
                  </div>
                  <!--end::Due-->
                  <!--begin::Due-->
                  <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-7 mb-3">
                    <div class="fs-6 text-gray-800 fw-bolder"><%=l @contract.start_date %></div>
                    <div class="fw-bold text-gray-600"><%= t('partner_contract.start_date')%></div>
                  </div>
                  <!--end::Due-->
                  <!--begin::Due-->
                  <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-7 mb-3">
                    <div class="fs-6 text-gray-800 fw-bolder"><%=l @contract.end_date %></div>
                    <div class="fw-bold text-gray-600"><%= t('partner_contract.end_date') %></div>
                  </div>
                  <!--end::Due-->
                  <!--begin::Due-->
                  <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-7 mb-3">
                    <div class="fs-6 text-gray-800 fw-bolder"><%= get_admin_plan_name(@contract.plan_id) %></div>
                    <div class="fw-bold text-gray-600"><%= t('partner_contract.partner_plan_id') %></div>
                  </div>
                  <!--end::Due-->
                  <!--begin::Due-->
                  <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-7 mb-3">
                    <div class="fs-6 text-gray-800 fw-bolder">
                    <%= @contract.payment_type.nil? ? '' : t("payment_type.#{@contract.payment_type}") %>
                    </div>
                    <div class="fw-bold text-gray-600"><%= t('partner_contract.payment_type') %></div>
                  </div>
                  <!--end::Due-->
                  
                </div>
              </div>
            </div>
          </div>
          <!--end::Content-->
        </div>
        <!--end::Layout-->
      </div>
      <!--end::Body-->
    </div>
    <!--end::Post card-->
  </div>
  <!--end::Container-->
</div>
<!--end::Content-->