<!--begin::Header-->
<% breadcrumbs_navigation_add  (t('partner.dashboard.partner_member_details')), '#' %>
<%= render_partner_breadcrumbs_navigation (t('partner.dashboard.partner_member_details')) %>		
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
  	<!--begin::Container-->
  	<div class="container-fluid" id="kt_content_container">
		<div class="d-flex flex-column flex-lg-row">
			<!--begin::Content-->
			<div class="flex-lg-row-fluid me-lg-15 order-2 order-lg-1 mb-10 mb-lg-0">
				<!--begin::Card-->
				<div class="card bg-body card-flush pt-3 mb-5 mb-xl-10">
					<!--begin::Card header-->
					<div class="card-header">
						<!--begin::Card title-->
						<div class="card-title">
							<h2 class="fw-bolder"><%= t('partner.dashboard.partner_member_details') %></h2>
						</div>
						<!--begin::Card title-->
                        <!--begin::Card toolbar-->
						<% if can?(:edit_partner_member_setup, Kodawarione::Partner) %>
							<div class="card-toolbar">
								<%= link_to t('super_partner.steps.buttons.back'), kodawarione_partner_manage_partner_details_path(@member.partners_id), :class=>"btn btn-light-primary me-3" %>
								<%= link_to t('btn.edit'), kodawarione_partner_manage_edit_partner_member_setup_path(id: @member.id,partner_id: @member.partners_id), :class=>"btn btn-primary" %>
							</div>
						<% end %>
						<!--end::Card toolbar-->
					</div>
					<!--end::Card header-->
					<!--begin::Card body-->
					<div class="card-body pt-3">
						<!--begin::Section-->
						<div class="mb-10">
							<!--begin::Title-->
							<h5 class="mb-4"><%= t('partner.dashboard.partner_member_details') %>:</h5>
							<!--end::Title-->
							<!--begin::Details-->
							<div class="d-flex flex-wrap py-5">
								<!--begin::Row-->
								<div class="flex-equal me-5">
									<!--begin::Details-->
									<table class="table fs-6 fw-bold gs-0 gy-2 gx-2 m-0">
										<!--begin::Row-->
										<tr>
											<td class="text-gray-600 min-w-175px w-175px"><%= t('partner.partner_user.title.name') %>:</td>
											<td class="text-gray-800 min-w-200px">
                                            <span class="text-gray-800 fw-bolder"><%= @admin.last_name + ' ' + @admin.first_name %></span>
											</td>
										</tr>
										<!--end::Row-->
										<!--begin::Row-->
                                        <tr>
                                            <td class="text-gray-600"><%= t('partner.partner_user.title.email') %>:</td>
                                            <td class="text-gray-800 fw-bolder"><%= @admin.email %></td>
                                        </tr>
                                        <!--end::Row-->
									</table>
									<!--end::Details-->
								</div>
								<!--end::Row-->
							</div>
							<!--end::Row-->
						</div>
						<!--end::Section-->
					</div>
					<!--end::Card body-->
				</div>
				<!--end::Card-->
			</div>
			<!--end::Content-->
		</div>
		<!--end::Layout-->
	</div>
	<!--end::Post-->
</div>
<!--end::Container-->