<script type="text/javascript">
  var BASE_URL = '<%= root_url %>';
</script>
<!--begin::Header-->
<% breadcrumbs_navigation_add (t('partner_plan.partner_plan_tab')), '#' %>
<%= render_partner_breadcrumbs_navigation(t ('partner_plan.partner_plan_tab')) %>  	
<!--end::Header-->
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Container-->
    <div class="container-fluid" id="kt_content_container">
        <!--begin::Form-->
        <%= form_with(url: kodawarione_plans_path, method: :get) do |form| %>
        <% end %>
        <!--end::Form-->
        <div class="tab-content">
			    <!--begin::Tab pane-->
          <div id="kt_project_users_card_pane" class="tab-pane fade show active">
            <!--begin::Table-->
            <div class="card bg-body p-10">
              <!--begin::Header-->
              <div class="card-header border-0 pb-7 px-0">
                <h3 class="fw-bolder me-5 my-1">
                  <span class="text-primary me-2"><%= t ('partner_plan.partner_plan_tab') %></span> <%= @plans.total_count %> <%= t ('search.count_unit') %>
                </h3>
                <div class="card-toolbar">
                  <% if can?(:create, Kodawarione::Plan) %>
                    <%= link_to t('partner_plan.plan_new'), new_kodawarione_plan_path, class: "btn btn-primary" %>
                  <% end %>
                </div>
              </div>
                <!--end::Header-->
                <!--begin::Body-->
                <div class="row">
                    <% if @plans.present? %>
                      <!--begin::Table container-->
                      <div class="table-responsive mt-0">
                        <!--begin::Table-->
                        <table class="table align-middle gs-0 gy-4">
                            <!--begin::Table head-->
                            <thead>
                                <tr class="fw-bolder text-muted bg-light">
                                    <th class="min-w-10px ps-4 min-w-10px rounded-start" scope="col"><%=  Partner::PartnerPlan.human_attribute_name('name') %></th>
                                    <th class="min-w-10px" scope="col"><%=  Partner::PartnerPlan.human_attribute_name('fee') %></th>
                                    <th class="min-w-70px" scope="col"><%= Partner::PartnerPlan.human_attribute_name('content') %></th>
                                    <th class="min-w-125px text-end rounded-end"> </th>
                                </tr>
                            </thead>
                            <!--end::Table head-->
                            <!--begin::Table body-->
                            <tbody>
                              <% @plans.each_with_index do |plan,index| %>
                              <tr>
                                  <td class="text-break" ><%= plan.name %></td>
                                  <td class="text-break" ><%= number_with_delimiter(plan.fee) %> å†† </td>
                                  <td class="text-break" ><%= plan.content %></td>
                                  <td class="text-end">
                                    <% if can?(:read, Kodawarione::Plan) %>
                                      <%= link_to kodawarione_plan_path(:id => plan.id), :class=> 'btn btn-icon btn-bg-light btn-active-color-primary btn-sm mr-3' do %>
                                          <span class="svg-icon svg-icon-3">
                                              <!--begin::Svg Icon | path:assets/media/svg/icons/General/Settings-1.svg-->
                                              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                                                  <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                                      <rect x="0" y="0" width="24" height="24" />
                                                      <path d="M7,3 L17,3 C19.209139,3 21,4.790861 21,7 C21,9.209139 19.209139,11 17,11 L7,11 C4.790861,11 3,9.209139 3,7 C3,4.790861 4.790861,3 7,3 Z M7,9 C8.1045695,9 9,8.1045695 9,7 C9,5.8954305 8.1045695,5 7,5 C5.8954305,5 5,5.8954305 5,7 C5,8.1045695 5.8954305,9 7,9 Z" fill="#000000" />
                                                      <path d="M7,13 L17,13 C19.209139,13 21,14.790861 21,17 C21,19.209139 19.209139,21 17,21 L7,21 C4.790861,21 3,19.209139 3,17 C3,14.790861 4.790861,13 7,13 Z M17,19 C18.1045695,19 19,18.1045695 19,17 C19,15.8954305 18.1045695,15 17,15 C15.8954305,15 15,15.8954305 15,17 C15,18.1045695 15.8954305,19 17,19 Z" fill="#000000" opacity="0.3" />
                                                  </g>
                                              </svg>
                                              <!--end::Svg Icon-->
                                          </span>
                                      <% end %> 
                                    <% end %>
                                    <% if can?(:update, Kodawarione::Plan) %>
                                      <%= link_to edit_kodawarione_plan_path(:id => plan.id), :class=> 'btn btn-icon btn-bg-light btn-active-color-primary btn-sm' do %>
                                          <span class="svg-icon svg-icon-3">
                                              <!--begin::Svg Icon | path:assets/media/svg/icons/Communication/Write.svg-->
                                              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                                                  <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                                      <rect x="0" y="0" width="24" height="24" />
                                                      <path d="M12.2674799,18.2323597 L12.0084872,5.45852451 C12.0004303,5.06114792 12.1504154,4.6768183 12.4255037,4.38993949 L15.0030167,1.70195304 L17.5910752,4.40093695 C17.8599071,4.6812911 18.0095067,5.05499603 18.0083938,5.44341307 L17.9718262,18.2062508 C17.9694575,19.0329966 17.2985816,19.701953 16.4718324,19.701953 L13.7671717,19.701953 C12.9505952,19.701953 12.2840328,19.0487684 12.2674799,18.2323597 Z" fill="#000000" fill-rule="nonzero" transform="translate(14.701953, 10.701953) rotate(-135.000000) translate(-14.701953, -10.701953)" />
                                                      <path d="M12.9,2 C13.4522847,2 13.9,2.44771525 13.9,3 C13.9,3.55228475 13.4522847,4 12.9,4 L6,4 C4.8954305,4 4,4.8954305 4,6 L4,18 C4,19.1045695 4.8954305,20 6,20 L18,20 C19.1045695,20 20,19.1045695 20,18 L20,13 C20,12.4477153 20.4477153,12 21,12 C21.5522847,12 22,12.4477153 22,13 L22,18 C22,20.209139 20.209139,22 18,22 L6,22 C3.790861,22 2,20.209139 2,18 L2,6 C2,3.790861 3.790861,2 6,2 L12.9,2 Z" fill="#000000" fill-rule="nonzero" opacity="0.3" />
                                                  </g>
                                              </svg>
                                              <!--end::Svg Icon-->
                                          </span>
                                      <% end %>                                   
                                    <% end %>                                   
                                  </td>
                                </tr>
                              <% end %>
                            </tbody>
                            <!--end::Table body-->
                        </table>
                        <!--end::Table-->
                      </div>
                      <!--begin::pagination-->
                            <div class="d-flex flex-wrap justify-content-between align-items-center">
                                <div class="d-flex flex-wrap py-2 me-3">
                                    <%= paginate @plans %>
                                </div>
                                <div class="d-flex align-items-center py-3">
                                    <span class="text-muted"><%=  t('common.display')  %> <%= @plans.total_count %> <%=  t('common.to')  %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <%= @plans.offset_value + 1 %> ~ <%=  @plans.offset_value + @plans.count %> <%=  t('common.search_result')  %> </span>
                                </div>
                            </div>
                            <!--end::pagination-->
                <!--end::Table container-->
                <% else %>
                  <div class="col-12 m-2">
                      <h3><%= t('search.no_result') %></h3>
                  </div>
                <% end %>
              </div>
            <!--begin::Body-->
          </div>
          <!--end::Table-->
			</div>
		</div>
  </div>
    <!--end::Container-->
</div>
<%= javascript_pack_tag "admin/admin_search.js" %>
<%= javascript_pack_tag 'common.js'%>
<%= javascript_pack_tag "student/event_search.js" %>
