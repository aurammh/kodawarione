				<!--begin::Table-->
				<div class="card bg-body">
					<!--begin::Header-->
					<div class="card-header border-0 pt-5">
						<h3 class="card-title align-items-start flex-column">
							<span class="card-label fw-bolder fs-3 mb-1"><%= t ('search.search_title') %></span>
						</h3>
						<div class="card-toolbar">
							<span class="card-label fs-3 mb-1"><%= result.total_count %><%= t ('search.count_unit') %></span>
						</div>
					</div>
					<!--end::Header-->
					<!--begin::Body-->
					<div class="card-body py-3">
                    <% if result.present? %>
						<!--begin::Table container-->
						<div class="table-responsive">
							<!--begin::Table-->
							<table class="table align-middle gs-0 gy-4">
								<!--begin::Table head-->
								<thead>
									<tr class="fw-bolder text-muted bg-light">
										<th class="ps-4 min-w-10px rounded-start" scope="col"><%= t('student.student_show.name') %></th>
                                        <th class="min-w-10px" scope="col"><%= t "unit.age" %></th>
                                        <th class="min-w-10px" scope="col"><%= Student::Student.human_attribute_name('gender') %></th>
                                        <th class="min-w-10px" scope="col"><%= Student::Student.human_attribute_name('school_name') %> </th>
                                        <th class="min-w-10px" scope="col"><%= Student::Student.human_attribute_name('school_type') %></th>
                                        <th class="min-w-10px" scope="col"><%= t('search.admin_prefecture_area') %></th>
                                        <th class="min-w-10px" scope="col"><%= Student::Student.human_attribute_name('club_name') %></th>
                                        <th class="min-w-10px" scope="col"><%= t('admin.setting.assessment_title') %></th>
                                        <th class="min-w-10px" scope="col"><%= t('search.date_label') %></th>
                                        <th class="min-w-10px"><%= t('event.title.publish_status') %></th>
                                        <th class="min-w-125px text-end rounded-end"></th>
									</tr>
								</thead>
								<!--end::Table head-->
								<!--begin::Table body-->
								<tbody>
									<% result.each do |user| %>
