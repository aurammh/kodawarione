                                                <div class="symbol symbol-circle symbol-50px overflow-hidden me-3">                                                
                                                    <div class="symbol-label">
                                                        <% if user.image.attached? %>
                                                            <%= image_tag user.image, :class=> "w-100" %>  
                                                        <% else %>
                                                            <% if user.gender == "female" %>
                                                                <%= image_tag "avatar/female.svg", class: "w-100" %>                                
                                                            <% else %>
                                                                <%= image_tag "avatar/male.svg", class: "w-100" %>   
                                                            <% end %>                                                   
                                                        <% end %>
                                                    </div>                                              
                                                </div>
                                                <!--end::Avatar-->
                                                <!--begin::User details-->
                                                <div class="d-flex flex-column">
                                                    <span class="text-gray-800 text-hover-primary mb-1">  <%= user.last_name %> <%= user.first_name %></span>
                                                </div>
                                                <!--begin::User details-->
                                            </td>
                                            <!--end::User=-->		
											<td>
												<span>
                                                <% unless user.birthday.nil?%>
                                                <%= Date.today.year - user.birthday.year%>
                                                <% end %>
                                                </span>
											</td>
											<td>
                                                <% unless user.gender.nil?%>
                                                <%= t("student.gender.#{user.gender}") %>
                                                <% end %>
                                            </td>
											<td>
                                                <%= user.school_name%>
                                            </td>
                                            <td>
                                                <span>
                                                    <% unless user.school_type.nil?%>
                                                        <%=  t("student.school_type.#{user.school_type}") %>
                                                    <% end %>
                                                </span>
                                            </td>
                                            <td>
                                                <span>
                                                    <%= (user.pre_name) %>
                                                </span>
                                            </td>
                                            <td>
                                                <span>
                                                    <%= (user.club_name) %>
                                                </span>
                                            </td>
                                            <td> <%= assessment.include?(user.id)? t('admin.setting.implemented') : t('admin.setting.not_implemented')%> </td>
											<td>
                                                <span>
