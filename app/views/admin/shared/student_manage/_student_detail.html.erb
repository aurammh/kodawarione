                                            <!--begin::Info-->
                                            <div class="fw-semibold ms-5">
                                                <!--begin::Title-->
                                                <span class="fs-5 fw-bold text-dark text-hover-primary mb-2"> <%= Student::Student.human_attribute_name('job_info')  %></span>
                                                <!--end::Title-->
                                                <!--begin::User-->
                                                <% unless student.job_info.nil? || student.job_info == "" %>
                                                    <div class="fs-7 text-muted"><%= student.job_info %></div>
                                                <% else %>
                                                    <div class="fs-7 text-muted">ない</div>
                                                <% end %>
