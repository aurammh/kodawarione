                <div class="card bg-body mb-5 mb-xl-10">
                    <!--begin::Card body-->
                    <div class="card-body">
                        <!--begin::Summary-->
                        <!--begin::User Info-->
                        <div class="d-flex flex-center flex-column py-5">
                            <!--begin::Avatar-->
                            <div class="symbol symbol-100px symbol-circle mb-7">
                                <% if student.image.attached? %>
                                    <%= image_tag (student.image), :class=> "Responsive image" %>
                                <% else %>
                                    <% if student.gender == "female" %>
                                        <%= image_tag "avatar/female.svg", :class=> "Responsive image" %>                                
                                    <% else %>
                                        <%= image_tag "avatar/male.svg", :class=> "Responsive image" %>   
                                    <% end %>
                                <% end %> 
                            </div>
