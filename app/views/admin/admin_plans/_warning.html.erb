<div class="modal fade pl-0" id="delete_news" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content ">
            <div class="modal-header admin-sidebar-color text-white">
                <h3 class="modal-title" id="exampleModalLongTitle"><%= t('plan.warning') %></h3>
                <button type="button" class="close text-white btn-close-white" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body company_type">
                <section>
                    <div class="text-left py-3">
                        <h5 class="text-dark font-weight-bold pl-1"><%= t('plan.warning_text') %></h5>
                    </div>
                    <div class="mt-3 d-flex justify-content-end">
                        <button type="button" class="btn-secondary text-white font-weight-bold col-sm-6 col-md-4 col-lg-2 custom-btn px-2 py-2 text-decoration-none ml-3" data-dismiss="modal"><%= t('role.title.return') %></button>
                    </div>
                </section>
            </div>
        </div>
    </div>
</div>