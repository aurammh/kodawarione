                </svg>
            </span>
            <!--end::Svg Icon-->
        </div>
        <!--end::Heaeder menu toggle-->
        <% if check_user_type == LogoutHistory.active_types["user"].to_i %>
            <%= link_to student_students_path(id: current_user.id) ,class: "" do %>
                <%= image_tag "layout/header_logo.png", class: "h-25px h-lg-40px" %>   
            <% end %>
        <% end %> 
        </a>
    </div>
