                                            <path d="M21,17.3570374 L21,21 C21,21.5522847 20.5522847,22 20,22 L4,22 C3.44771525,22 3,21.5522847 3,21 L3,17.4976746 C3.098145,17.5882704 3.2035241,17.6804734 3.31568417,17.7726326 C4.24088818,18.5328503 5.30737928,19 6.5,19 C7.52608715,19 8.26628185,18.7060277 9.33838848,18.0385822 C9.41243034,17.9924871 9.49377318,17.9413176 9.64386645,17.8468046 C10.6511414,17.2141042 11.1835561,17 12,17 C12.7988191,17 13.2700619,17.2056332 14.0993283,17.8087361 C14.2431314,17.9137812 14.3282387,17.9759674 14.3943239,18.0236679 C15.3273176,18.697107 16.0099741,19 17,19 C18.3748985,19 19.7104312,18.4390637 21,17.3570374 Z" fill="#000000" opacity="0.3"/>
                                        </g>
                                    </svg>
								</span>
								<!--end::Svg Icon-->
								<%= l @user_data.birthday if @user_data.birthday.present? %>
							</span>
							<span class="d-flex align-items-center text-gray-400 me-5 mb-2">
								<!--begin::Svg Icon | path: icons/duotune/communication/com011.svg-->
								<span class="svg-icon svg-icon-4 me-1">
									<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
										<path opacity="0.3" d="M21 19H3C2.4 19 2 18.6 2 18V6C2 5.4 2.4 5 3 5H21C21.6 5 22 5.4 22 6V18C22 18.6 21.6 19 21 19Z" fill="black" />
										<path d="M21 5H2.99999C2.69999 5 2.49999 5.10005 2.29999 5.30005L11.2 13.3C11.7 13.7 12.4 13.7 12.8 13.3L21.7 5.30005C21.5 5.10005 21.3 5 21 5Z" fill="black" />
									</svg>
								</span>
								<!--end::Svg Icon--><%= current_user.email %>
							</span>
                            <span class="<%= 'd-none' unless @user_data.display_address.present? %> d-flex align-items-center text-gray-400 text-hover-primary mb-2">
								<!--begin::Svg Icon | path: icons/duotune/communication/com006.svg-->
								<span class="svg-icon svg-icon-4 me-1">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
										<path opacity="0.3" d="M18.0624 15.3453L13.1624 20.7453C12.5624 21.4453 11.5624 21.4453 10.9624 20.7453L6.06242 15.3453C4.56242 13.6453 3.76242 11.4453 4.06242 8.94534C4.56242 5.34534 7.46242 2.44534 11.0624 2.04534C15.8624 1.54534 19.9624 5.24534 19.9624 9.94534C20.0624 12.0453 19.2624 13.9453 18.0624 15.3453Z" fill="black" />
										<path d="M12.0624 13.0453C13.7193 13.0453 15.0624 11.7022 15.0624 10.0453C15.0624 8.38849 13.7193 7.04535 12.0624 7.04535C10.4056 7.04535 9.06241 8.38849 9.06241 10.0453C9.06241 11.7022 10.4056 13.0453 12.0624 13.0453Z" fill="black" />
                                    </svg>
								</span>
								<!--end::Svg Icon--><%= @user_data.display_address %>
							</span>
						</div>
						<!--end::Info-->
					</div>
					<!--end::User-->
				</div>
				<!--end::Title-->
			</div>
			<!--end::Info-->
		</div>
		<!--end::Details-->
		<!--begin::Navs-->
		<div class="d-flex overflow-auto h-55px">
			<ul class="nav nav-stretch nav-line-tabs nav-line-tabs-2x border-transparent fs-5 fw-bolder flex-nowrap">
				<!--begin::Nav item-->
				<li class="nav-item">
                    <%= link_to (t 'student_commitment.title.student_commitment_title1'), edit_student_student_commitment_path(@user_data), :class=> "nav-link text-active-primary me-6 #{current_route == "commitment" && 'active'}" %>
				</li>
				<!--end::Nav item-->
				<!--begin::Nav item-->
				<li class="nav-item">
                    <%= link_to (t 'common.registration_confirm_screen1'), edit_student_student_path(@user_data), :class=> "nav-link text-active-primary me-6 #{current_route == "profile" && 'active'}" %>
				</li>
				<!--end::Nav item-->
				<!--begin::Nav item-->
				<li class="nav-item">
                    <%= link_to (t 'password_setting.title.change_login_setting'), user_setting_path(@user_data), :class=> "nav-link text-active-primary me-6 #{current_route == "password" && 'active'}" %>
				</li>
				<!--end::Nav item-->
			</ul>
		</div>
		<!--begin::Navs-->
	</div>
</div>