<!--begin::Breadcrumb-->
<div class="subheader py-2 py-lg-6 subheader-transparent" id="kt_subheader">
  <div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
    <% breadcrumbs_navigation_add (t ('common.registration_confirm_screen')), '#' %>
    <%= render_student_breadcrumbs_navigation(t ('common.registration_confirm_screen')) %>	
  </div>
</div>
<!--end::Breadcrumb-->
<%= form_with(model: company_commitment_profile) do |form| %>
  <!--begin::Entry-->
  <div class="d-flex flex-column-fluid">
    <!--begin::Container-->
    <div class="container">
      <!--begin::Card-->
      <div class="card card-custom card-sticky" id="kt_page_sticky_card">
        <div class="card-header">
          <div class="card-title">
            <h3 class="card-label"><%=t('common.registration_confirm_screen')%></h3>
          </div>
          <div class="card-toolbar">
            <%= link_to root_path, class: "btn btn-light-info font-weight-bolder mr-2" do %> 
              <i class="ki ki-long-arrow-back icon-sm"></i><%= t('btn.cancel') %>
            <% end %>
            <%= button_tag( :class => "btn btn-info font-weight-bolder") do %>
              <i class="ki ki-check icon-sm"></i> <%= t('btn.submit') %> 
            <% end %>
          </div>
        </div>
        <div class="card-body">
            <div class="form-group row">
              <div class="col-12 col-sm-12 col-md-5  col-lg-5 col-xl-5 text-left text-sm-left text-md-right text-lg-right text-xl-right col-form-label font-weight-bold pr-0" >
                <%= form.label :title_1 %>
              </div>
              <div class="col-12 col-sm-12 col-md-6 col-lg-5 col-xl-5">
                <%= form.text_field :title_1, class: "form-control", id: "title_1",  maxlength: 200 %>
              </div>
            </div>
            <div class="form-group row">
              <%= form.hidden_field :haveFileOneFlag, :value => false , id: "haveFileOneFlag" %>
              <div class="col-12 col-sm-12 col-md-5  col-lg-5 col-xl-5 text-left text-sm-left text-md-right text-lg-right text-xl-right col-form-label font-weight-bold pr-0" ></div>
              <%= form.file_field :title_1_video,accept: 'video/mp4,video/x-m4v,video/*', :class=> "d-none", :id => "uploadVideoOne","data-existed": form.object.title_1_video.attached? %>
              <div class="col-12 col-sm-12 col-md-6 col-lg-5 col-xl-5">
                <div class="form-control">
                  <label for="uploadVideoOne" class="truncate">
                    <% if form.object.title_1_video.attached? && form.object.title_1_video.persisted? %>
                      <span class="file-chosen-one"><%= form.object.title_1_video.filename %></span>
                    <% else %>
                      <span class="file-chosen-one"><%= t('student.title.pdf_text') %></span>
                    <% end %>
                  </label>
                  <button type="button" class="close" id="remove_file_one">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
              </div>
            </div>
            <div class="form-group row">
              <div class="col-12 col-sm-12 col-md-5  col-lg-5 col-xl-5 text-left text-sm-left text-md-right text-lg-right text-xl-right col-form-label font-weight-bold pr-0" >
                <%= form.label :title_2 %>
              </div>
              <div class="col-12 col-sm-12 col-md-6 col-lg-5 col-xl-5">
                <%= form.text_field :title_2, class: "form-control", id: "title_2",  maxlength: 200 %>
              </div>
            </div>
            <div class="form-group row">
              <%= form.hidden_field :haveFileTwoFlag, :value => false , id: "haveFileTwoFlag" %>
              <div class="col-12 col-sm-12 col-md-5  col-lg-5 col-xl-5 text-left text-sm-left text-md-right text-lg-right text-xl-right col-form-label font-weight-bold pr-0" ></div>
              <%= form.file_field :title_2_video,accept: 'video/mp4,video/x-m4v,video/*', :class=> "d-none", :id => "uploadVideoTwo","data-existed": form.object.title_2_video.attached? %>
              <div class="col-12 col-sm-12 col-md-6 col-lg-5 col-xl-5">
                <div class="form-control">
                  <label for="uploadVideoTwo" class="truncate">
                    <% if form.object.title_2_video.attached? && form.object.title_2_video.persisted? %>
                      <span class="file-chosen-two"><%= form.object.title_2_video.filename %></span>
                    <% else %>
                      <span class="file-chosen-two"><%= t('student.title.pdf_text') %></span>
                    <% end %>
                  </label>
                  <button type="button" class="close" id="remove_file_two">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
              </div>
            </div>
            <div class="form-group row">
              <div class="col-12 col-sm-12 col-md-5  col-lg-5 col-xl-5 text-left text-sm-left text-md-right text-lg-right text-xl-right col-form-label font-weight-bold pr-0" >
                <%= form.label :title_3 %>
              </div>
              <div class="col-12 col-sm-12 col-md-6 col-lg-5 col-xl-5">
                <%= form.text_field :title_3, class: "form-control", id: "title_3",  maxlength: 200 %>
              </div>
            </div>
            <div class="form-group row">
              <%= form.hidden_field :haveFileThreeFlag, :value => false , id: "haveFileThreeFlag" %>
              <div class="col-12 col-sm-12 col-md-5  col-lg-5 col-xl-5 text-left text-sm-left text-md-right text-lg-right text-xl-right col-form-label font-weight-bold pr-0" ></div>
              <%= form.file_field :title_3_video,accept: 'video/mp4,video/x-m4v,video/*', :class=> "d-none", :id => "uploadVideoThree","data-existed": form.object.title_3_video.attached? %>
              <div class="col-12 col-sm-12 col-md-6 col-lg-5 col-xl-5">
                <div class="form-control">
                  <label for="uploadVideoThree" class="truncate">
                    <% if form.object.title_3_video.attached? && form.object.title_3_video.persisted? %>
                      <span class="file-chosen-three"><%= form.object.title_3_video.filename %></span>
                    <% else %>
                      <span class="file-chosen-three"><%= t('student.title.pdf_text') %></span>
                    <% end %>
                  </label>
                  <button type="button" class="close" id="remove_file_three">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
              </div>
            </div>
        </div>
      </div>
      <!--end::Card-->
    </div>
    <!--end::Container-->
  </div>
  <!--end::Entry-->
  <div class="position-fixed bottom-0 right-0 p-3" style="z-index: 5; right: 0; bottom: 0;">
    <div id="liveToast" class="toast border border-4 border-success hide" role="alert" aria-live="assertive" aria-atomic="true" data-delay="2000">
      <div class="toast-header">
        <%= favicon_link_tag asset_path('layout/favicon.ico') %>   
        <strong class="mr-auto"><%= t('genuine_password.toast_title') %></strong>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="toast-body">
        <%= t('genuine_password.toast_body') %>
      </div>
    </div>
  </div>   
  <input type= "hidden" value = <%= @genuine_password_flag %> id = 'genuine_password' >
<% end %>
<%= render "users/shared/toastr" %>
<%= javascript_pack_tag "company/commitment_profile.js" %>